#include <stdio.h>
#include <stdlib.h>

void main(){

    double valor1, valor2, valor3, media;

    scanf("%lf %lf %lf", &valor1, &valor2, &valor3);

    media = (valor1+valor2+valor3)/3;

    if(media >= 7)
        printf("aprovado\n");
    else if(media < 3)
        printf("reprovado\n");
    else if(media >=3 && media < 7)
        printf("prova final\n");

}
