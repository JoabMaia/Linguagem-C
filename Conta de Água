#include<stdio.h>
#include<stdlib.h>
#include<string.h>


int main ( )
{
    int n =0, conta =7, verifica =0;
    scanf("%d", &n);


    if (n > 100) {
        verifica = n - 100;
        n -=verifica;
        conta += ( verifica * 5);
    }

    if (n > 30) {
        verifica = n - 30;
        n -= verifica;
        conta += verifica * 2;
    }

    if (n > 10) {
        verifica = n - 10;
        conta += verifica;
    }

    printf("%d", conta);

    return 0 ;
}
