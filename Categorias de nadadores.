#include<stdio.h>
#include<string.h>

int main(){

    int idade;
    scanf("%d", &idade);

    if (idade >= 5 && idade <= 7)
        printf("Infantil A\n");

    if (idade >= 8 && idade <= 10)
        printf("Infantil B\n");

    if (idade >= 11 && idade <= 13)
        printf("Juvenil A\n");

    if (idade >= 14 && idade <= 17)
      printf("Juvenil B\n");

    if (idade >= 18 && idade <= 40)
      printf("Adulto\n");

    if (idade > 40)
      printf("Master\n");

    if (idade < 5)
        printf("Idade invalida.\n");


    return 0;
}
