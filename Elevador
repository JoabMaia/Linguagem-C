#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main ( )
{
    int n=0, c =0, s=0, e =0, total =0, cont =0, i;

    scanf("%d %d", &n, &c);
    for (i =0; i < n ; i ++){

        scanf("%d %d", &s, &e);

        total += (e-s);

        if (total > c){
            cont++;
        }
    }
    if (cont>= 1 ){
        printf("S\n");
    }else{
        printf("N\n");
    }
    
    return 0 ;
}
