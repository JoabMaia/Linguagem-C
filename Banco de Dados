#include<stdio.h>
#include<string.h>

int main(){

    int entrada, idade, quantidade_amigos, quantidade_fotos, i;
    char nome[50], sexo, estado_civil;
    scanf("%d", &entrada);

    for(i=0; i<entrada; i++){
        scanf("%d %s %c %c %d %d", &idade, nome, &sexo, &estado_civil, &quantidade_amigos, &quantidade_fotos);
        printf("Idade: %d\n", idade);
        printf("Nome: %s\n", nome);
        printf("Sexo: %c\n", sexo);
        printf("Estado Civil: %c\n", estado_civil);
        printf("Numero de amigos: %d\n", quantidade_amigos);
        printf("Numero de fotos: %d\n", quantidade_fotos);
        printf("\n");
    }

    return 0;
}
