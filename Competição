#include<stdio.h>
#include<string.h>

int main(){

    int valores[3], valor, i, maior;

    for(i=0; i<3; i++){
        scanf("%d", &valores[i]);
    }
    scanf("%d", &valor);
    maior = valores[0];
    for(i=0; i<3; i++){
        if (valores[i] > maior){
            maior = valores[i];
        }
    }
    printf("%d", maior * valor);
    return 0;
}
